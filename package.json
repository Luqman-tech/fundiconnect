{
    "name": "fundiconnect",
    "version": "1.0.0",
    "description": "FundiConnect - Find & Book Trusted Service Providers in Kenya",
    "main": "index.html",
    "scripts": {
       
        "start": "npx http-server . -p 3000",
        "dev": "npx live-server .",
        "build": "npm run build:backend && npm run build:frontend",
        "build:backend": "echo 'Backend build complete'",
        "build:frontend": "cd frontend && npm run build",
        "deploy": "netlify deploy --prod --dir .",
        "deploy:staging": "npm run build && npm run deploy:aws:staging",
        "deploy:production": "npm run build && npm run deploy:aws:production",
        "deploy:aws:staging": "aws s3 sync frontend/dist s3://staging-fundiconnect",
        "deploy:aws:production": "aws s3 sync frontend/dist s3://production-fundiconnect",
        "test": "jest",
        "test:unit": "jest --testPathPattern=unit",
        "test:integration": "jest --testPathPattern=integration",
        "test:e2e": "jest --testPathPattern=e2e",
        "test:coverage": "jest --coverage",
        "test:watch": "jest --watch",
        "test:ci": "jest --ci --coverage --watchAll=false",
        "db:setup": "node scripts/setup-database.js",
        "db:migrate": "npx sequelize-cli db:migrate",
        "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
        "db:seed": "npx sequelize-cli db:seed:all",
        "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
        "db:reset": "npm run db:migrate:undo && npm run db:migrate && npm run db:seed",
        "db:test:setup": "node scripts/setup-test-database.js",
        "lint": "eslint src/ --ext .js,.ts",
        "lint:fix": "eslint src/ --ext .js,.ts --fix",
        "format": "prettier --write src/",
        "format:check": "prettier --check src/",
        "type-check": "tsc --noEmit",
        "webhook:start": "node src/services/whatsapp/webhook-server.js",
        "webhook:dev": "nodemon src/services/whatsapp/webhook-server.js",
        "webhook:test": "jest --testPathPattern=whatsapp"
    },
    "keywords": [
        "fundi",
        "services",
        "kenya",
        "booking",
        "whatsapp",
        "freelancers",
        "local-services"
    ],
    "author": "Your Name",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/yourusername/fundiconnect.git"
    },
    "homepage": "https://yourusername.github.io/fundiconnect",
    "dependencies": {
        "express": "^4.21.2",
        "cors": "^2.8.5",
        "helmet": "^7.2.0",
        "morgan": "^1.10.0",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.7",
        "express-rate-limit": "^7.5.1",
        "express-session": "^1.18.1",
        "connect-redis": "^7.1.1",
        "sequelize": "^6.37.7",
        "sequelize-cli": "^6.6.3",
        "pg": "^8.16.3",
        "sqlite3": "^5.1.7",
        "redis": "^4.7.1",
        "dotenv": "^16.6.1",
        "bcryptjs": "^2.4.3",
        "jsonwebtoken": "^9.0.2",
        "axios": "^1.10.0",
        "winston": "^3.17.0",
        "multer": "^1.4.5-lts.2",
        "moment": "^2.30.1",
        "moment-timezone": "^0.5.48",
        "joi": "^17.13.3",
        "uuid": "^8.0.0",
        "node-cron": "^3.0.3",
        "socket.io": "^4.8.1",
        "africastalking": "^0.6.7",
        "google-libphonenumber": "^3.2.42",
        "aws-sdk": "^2.1692.0",
        "date-fns": "^2.30.0"
    },
    "devDependencies": {
        "http-server": "^14.1.1",
        "live-server": "^1.2.2",
        "nodemon": "^3.1.10",
        "jest": "^29.7.0",
        "supertest": "^6.3.4",
        "eslint": "^8.57.1",
        "prettier": "^3.6.2"
    }
}